{
    "name": "zezont4/LaravelGenerator",
    "type": "library",
    "description": "Generate the main classes that laravel use to work with database . Like model,controller,request,blade forms",
    "keywords": [
        "zezont4",
        "LaravelGenerator"
    ],
    "homepage": "https://github.com/zezont4/LaravelGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulaziz Tayyar",
            "email": "zezont@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php": "~5.5|~7.0",
        "laravelcollective/html": "5.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            ":vendor\\:package_name\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            ":vendor\\:package_name\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
