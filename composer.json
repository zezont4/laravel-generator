{
    "name": "zezont4/laravel-generator",
    "type": "library",
    "description": "Generate the main classes that laravel uses to work with database . Like model,controller,request,blade forms ",
    "keywords": [
        "zezont4",
        "LaravelGenerator"
    ],
    "homepage": "https://github.com/zezont4/LaravelGenerator",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulaziz Tayyar",
            "email": "zezont@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravelcollective/html": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "Zezont4\\LaravelGenerator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "scripts": {
        "post-update-cmd": [
            "php artisan vendor:publish --force"
        ]
    }
}
